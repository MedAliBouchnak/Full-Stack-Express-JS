<section id="form-details" class="section-p1">
  
    <form id="form">
        <span>LEAVE A MESSAGE</span>
        <h2>We love to hear from you</h2>
        
        <input type="text" id="username" placeholder="Your Username" >
        <label for="text" id="nameLabel"></label>
        <br>
        <input type="email" id="email" placeholder="Write your E-mail">
        <label for="email" id="emailLabel"></label>
        <br>
        <input type="password" id="password" placeholder="Write your Password">
        <label for="password" id="passLabel"></label>
        <br>
        <textarea name="" id="" cols="30" rows="10" placeholder="Your Massage"></textarea>
        <button class="normal" type="submit">Submit</button>
    </form>
</section>
    <section id="newsLetter" class="section-p1 section-m1">
      <div class="newText">
